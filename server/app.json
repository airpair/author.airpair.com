{
  "auth": {
    "appKey":                  "athr",
    "oauth": {
      "github": {
        "signup":              false,
        "login":               true,
        "unlink":              false,
        "emails":              false,
        "logic":               "oauth",
        "userAgent":           "{{required}}",
        "scope":               ["public_repo"]
      }
    }
  },
  "http": {
    "static": {
      "bundle": {
        "appCss":             "/css/app.css",
        "appJs":              "/js/ang1.js",
        "libsCss":            "/css/libs.css"
      },
      "dirs":                 "public,public/bundle"
    }
  },
  "log": {
    "model":                  false,
    "auth":                   { "theme": { "link": "green" } }
  },
  "middleware": {
    "session":                { "authedData": "_id name" },
    "plugins":                ["res.slow","livereload"]
  },
  "model": {
    "sessionStore":           { "collection": "sessions_author" }
  }
}
