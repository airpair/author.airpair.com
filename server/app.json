{
  "auth": {
    "appKey":                  "athr",
    "oauth": {
      "github": {
        "signup":              false,
        "login":               true,
        "unlink":              false,
        "emails":              false,
        "logic":               "loginAuthor",
        "userAgent":           "{{required}}",
        "scope":               ["public_repo"]
      }
    }
  },
  "http": {
    "static": {
      "bundle": {
        "appCss":             "/css/app.css",
        "appJs":              "/js/ang1.js",
        "libsCss":            "/css/libs.css"
      },
      "dirs":                 "public,public/bundle"
    }
  },
  "log": {
    "model":                  false,
    "auth":                   { "theme": { "link": "green" } },
    "wrpr":                   { "theme": { "ghcall": "cyan" } }
  },
  "middleware": {
    "session":                { "authedData": "_id name" },
    "plugins":                ["res.slow","livereload"]
  },
  "model": {
    "sessionStore":           { "collection": "sessions_author" },
    "cache": {
      "tags":                 { "getter": "tags.getForCache" }
    }
  },
  "wrappers": {
    "gitPublisher": {
      "org":                  "{{required}}",
      "retry":                { "delay": 1000, "max": 3 }
    }
  }
}
