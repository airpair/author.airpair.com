{
  "auth": {
    "appKey":                  "athr",
    "oauth": {
      "github": {
        "signup":              false,
        "login":               true,
        "unlink":              false,
        "emails":              false,
        "logic":               "loginAuthor",
        "userAgent":           "{{required}}",
        "scope":               ["public_repo","user"]
      }
    }
  },
  "http": {
    "static": {
      "bundles": {
        "css/lib.css":         "/static.airpair.com/css/libs.css",
        "css/app.css":         "css/app.css",
        "js/ang1.js":          "js/ang1.js"
      },
      "dirs":                  "public,static"
    }
  },
  "log": {
    "model":                   false,
    "auth":                    { "link":"green" },
    "wrpr":                    { "ghcall":"cyan", "ghretry":"yellow", "ghstep":"white" }
  },
  "middleware": {
    "session":                 { "authedData": "_id name" },
    "plugins":                 ["res.slow","livereload"]
  },
  "model": {
    "analytics": {
      "mongoUrl":              "{{required}}"
    },
    "session":                 { "collection": "sessions_author" },
    "cache": {
      "tags":                  { "getter": "tags.getForCache" }
    }
  },
  "wrappers": {
    "dirs":                    "wrappers",
    "gitPublisher": {
      "adminToken":            "{{required}}",
      "org":                   "{{required}}",
      "retry":                 { "delay": 1000, "max": 3 }
    }
  }
}
